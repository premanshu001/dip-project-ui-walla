{
  "version": 2,
  "builds": [
    {
      "src": "./main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/(.+)", "dest": "/main.js" },
    { "src": "/", "dest": "/public/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [{ "key": "Access-Control-Allow-Origin", "value": "*" }]
    },
    {
      "source": "/vercel_app_domain_name/(.*)",
      "headers": [{ "key": "Access-Control-Allow-Origin", "value": "*" }]
    }
  ],
  "git": {
    "deployments": {
      "main": true,
      "dev": false
    }
  }
}
