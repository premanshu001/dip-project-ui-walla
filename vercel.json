{
  "version": 2,
  "builds": [
    {
      "src": "./main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "public",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": false
    }
  }
}
